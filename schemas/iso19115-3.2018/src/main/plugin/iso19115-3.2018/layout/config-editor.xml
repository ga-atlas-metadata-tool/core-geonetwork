    <for name="gml:duration" use="data-gn-field-duration-div"/>

    <for name="gmx:FileName/src" use="data-gn-logo-selector"/>

    <for name="cit:electronicMailAddress" use="email"/>

    <for name="lan:language" use="data-gn-language-picker"/>
