  <tableFields>
    <!-- This is an example for organisation table layout.
    19115-3 being more flexible with link between orgs and individual,
    a table view force some constraints. -->
    <table for="cit:CI_Responsibility">
      <header>
        <col label="cit:role"/>
        <col label="cit:name"/>
        <col label="cit:CI_Individual"/>
        <col label="cit:electronicMailAddress"/>
        <col/>
      </header>
      <row>
        <col xpath="cit:role"/>
        <col xpath="cit:party/*/cit:name"/>
        <col xpath="cit:party/cit:CI_Organisation/cit:individual/cit:CI_Individual/cit:name"/>
        <col xpath="cit:party/*/cit:contactInfo/*/cit:address/*/cit:electronicMailAddress"/>
        <col del=".."/>
      </row>
    </table>

    <table for="srv:serviceStandard">
      <header>
        <col label="cit:title"/>
        <col label="cit:onlineResource"/>
        <col/>
      </header>
      <row>
        <col xpath="cit:CI_Citation/cit:title"/>
        <col xpath="cit:CI_Citation/cit:onlineResource/cit:CI_OnlineResource/cit:linkage"/>
        <col del=".." />
      </row>
    </table>

    <table for="mrs:MD_ReferenceSystem">
      <header>
        <col label="mrs:referenceSystemType" />
        <col label="mcc:code" />
        <col label="mcc:description" />
        <col />
      </header>
      <row>
        <col xpath="mrs:referenceSystemType" />
        <col xpath="mrs:referenceSystemIdentifier/mcc:MD_Identifier/mcc:code" />
        <col xpath="mrs:referenceSystemIdentifier/mcc:MD_Identifier/mcc:description" />
        <col del=".." />
      </row>
    </table>

   <table for="mco:MD_SecurityConstraints">
      <header>
        <col label="mco:MD_SecurityConstraints" />
        <col label="mco:constraintApplicationScope" />
        <col />
      </header>
      <row>
        <col xpath="mco:reference/cit:CI_Citation/cit:title" />
        <col xpath="mco:constraintApplicationScope/mcc:MD_Scope/mcc:level" />
        <col del=".." />
      </row>
    </table>
   <table for="mco:MD_Constraints">
      <header>
        <col label="mco:useLimitation" />
        <col label="mco:constraintApplicationScope" />
        <col label="cit:linkage" />
      </header>
      <row>
        <col xpath="mco:useLimitation" />
        <col xpath="mco:constraintApplicationScope" />
        <col xpath="mco:reference/cit:CI_Citation/cit:onlineResource/cit:CI_OnlineResource/cit:linkage" />
        <col del=".." />
      </row>
    </table>
    <table for="mco:MD_LegalConstraints">
      <header>
        <col label="mco:MD_LegalConstraints" />
        <col label="mco:accessConstraints" />
        <col label="mco:useConstraints" />
        <col label="mco:constraintApplicationScope" />
        <col />
      </header>
      <row>
        <col xpath="mco:reference/cit:CI_Citation/cit:title" />
        <col xpath="mco:accessConstraints" />
        <col xpath="mco:useConstraints" />
        <col xpath="mco:constraintApplicationScope/mcc:MD_Scope/mcc:level" />
        <col del=".." />
      </row>
    </table>
    <table for="mco:MD_Releasability">
      <header>
        <col label="mco:MD_Releasability" />
        <col label="mco:disseminationConstraints" />
        <col label="mco:addressee" />
        <col />
      </header>
      <row>
        <col xpath="mco:statement" />
        <col xpath="mco:disseminationConstraints" />
        <col xpath="mco:addressee/cit:CI_Responsibility/cit:party/cit:CI_Organisation/cit:name" />
        <col del=".." />
      </row>
    </table>

    <table for="cit:CI_OnlineResource">
      <header>
        <col label="cit:linkage" />
        <col label="cit:protocol" />
        <col label="cit:function" />
        <col />
      </header>
      <row>
        <col xpath="cit:linkage" />
        <col xpath="cit:protocol" />
        <col xpath="cit:function" />
        <col del=".." />
      </row>
    </table>
  </tableFields>


  <!-- View configuration -->
  <views>

    <view name="icsm" class="gn-label-above-input" displayTooltips="true" hideTimeInCalendar="true" >
      <sidePanel>
        <directive data-gn-onlinesrc-list="" data-config-id="icsm" data-menu="false" />
        <directive gn-geo-publisher=""
                   data-ng-if="gnCurrentEdit.geoPublisherConfig"
                   data-config="{{gnCurrentEdit.geoPublisherConfig}}"
                   data-lang="lang"/>
        <!--<directive data-gn-suggestion-list=""/>-->
        <directive data-gn-validation-report=""/>
        <directive data-gn-need-help="https://openwork-nz.github.io/ANZLIC-Metadata-Tool-help/"/>
      </sidePanel>

      <tab id="default" default="true" mode="flat">
        <section name="generalIcsm">
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:citation/cit:CI_Citation/cit:title" />
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:abstract" />
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:purpose" />
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:supplementalInformation" />
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:topicCategory" />
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:status" or="status"
                 in="/mdb:MD_Metadata/mdb:identificationInfo/*" />
        </section>
        <section name="dataIdentifierIcsm" >
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:citation/cit:CI_Citation/cit:identifier"
                 or="identifier" in="/mdb:MD_Metadata/mdb:identificationInfo/*/mri:citation/cit:CI_Citation" />
        </section>
      </tab>
      <tab id="serviceIcsm" mode="flat" displayIfRecord="count(mdb:MD_Metadata/mdb:identificationInfo/srv:SV_ServiceIdentification) > 0">
        <section name="serviceIcsm">
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/srv:SV_ServiceIdentification/srv:serviceType"/>
          <field xpath="/mdb:MD_Metadata/mdb:identificationInfo/srv:SV_ServiceIdentification/srv:serviceTypeVersion" />
